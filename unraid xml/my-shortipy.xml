<?xml version="1.0"?>
<Container version="2">
  <Name>shortipy</Name>
  <Repository>knex666/shortipy</Repository>
  <Registry>https://hub.docker.com/repository/docker/knex666/shortipy</Registry>
  <Network>br0</Network>
  <MyIP>192.168.100.95</MyIP>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/maschhoff/shortipy</Project>
  <Overview>URL Shortener in python based on flask</Overview>
  <Category>Tools: Network:Management</Category>
  <WebUI>http://[IP]:4321/</WebUI>
  <TemplateURL/>
  <Icon>https://image.flaticon.com/icons/png/512/1828/1828108.png</Icon>
  <ExtraParams/>
  <PostArgs></PostArgs>
  <CPUset/>
  <DateInstalled>1588924194</DateInstalled>
  <DonateText>Buy me a Pizza</DonateText>
  <DonateLink>https://www.buymeacoffee.com/maschhoff</DonateLink>
  <Description>URL Shortener in python based on flask&#xD;
&#xD;
Runs on port 4321 if you use a bridge mode map this port to any of your choice</Description>
  <Networking>
    <Mode>br0</Mode>
    <Publish/>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/shortipy</HostDir>
      <ContainerDir>/source/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Path 1" Target="/source/data" Default="" Mode="rw" Description="Container Path: /source/data" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/shortipy</Config>
</Container>
